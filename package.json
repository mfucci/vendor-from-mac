{
  "name": "vendor-from-mac",
  "version": "0.0.1",
  "description": "Returns the IEEE registered vendor name from the MAC address, using an up-to-date database (script to update it automatically).",
  "main": "build/VendorFromMac.js",
  "scripts": {
    "build": "tsc",
    "build_test": "tsc --project tsconfig.test.json",
    "build_script": "tsc --project tsconfig.script.json",
    "update": "npm run build_script && node build/script/Update.js",
    "test": "npm run build && npm run build_test && mocha --require ts-node/register ./build/test/test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mfucci/vendor-from-mac.git"
  },
  "keywords": [
    "ieee",
    "mac",
    "vendor",
    "oui"
  ],
  "author": "Marco Fucci di Napoli <mfucci@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/mfucci/vendor-from-mac/issues"
  },
  "homepage": "https://github.com/mfucci/vendor-from-mac#readme",
  "devDependencies": {
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.18",
    "axios": "^0.26.0",
    "csv-parse": "^5.0.4",
    "mocha": "^9.2.0",
    "preferences": "^2.0.2",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {}
}
